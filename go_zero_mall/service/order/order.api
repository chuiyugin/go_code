// 定义类型
// 搜索相关
type SearchRequest {
	OrderID string `form:"orderID"`
}

type SearchResponse {
	OrderID  string `json:"orderID"`
	Status   int    `json:"status"`
	Username string `json:"username"`
}

@server (
	prefix: api
)
service order-api {
	@handler SearchHandler
	get /order/search (SearchRequest) returns (SearchResponse)
}

